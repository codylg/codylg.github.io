extends content

block variables
  - var classname = "attract"
  - var smallpanel = false

block header
  h1.white-text
    a(id="#{classname}", href="http://attract.ai", target="_blank") Attract.ai
  
block description
  p.white-text
    a(href="http://attract.ai", target="_blank") Attract.ai
    |  is a tech startup developing a platform to build and retain stronger teams.
  p.white-text
    | I designed the Attract.ai 
    strong brand
    |  and 
    strong developed 
    | their initial landing page and reports.
  hr.white-line
  a.button-white(href="http://attract.ai", target="_blank") http://attract.ai &rarr; 

block figure
  figure.image-gallery
    img(srcset="/img/attract-01.jpg 1x, /img/attract-01@2x.jpg 2x" alt="Attract.ai landing page")
    img(srcset="/img/attract-02.jpg 1x, /img/attract-02@2x.jpg 2x" alt="Attract.ai report")
